{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://cu257.cloud.maa.collab.net/sf/sfmain/do/home"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "username"
      },
      "text": "admin"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "pwd"
      },
      "text": "admin"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Log In"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "regression_boards"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "img[alt=\"Trackers\"]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Board View"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input.combo-text.validatebox-text"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "div.tree-node"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "i.icon-plus.add-artifact"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='tracker-list']//select[.='Nonetrack001']//option[2]"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "editor_new_title"
      },
      "text": "test002"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "id",
        "value": "copy-title"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Submit"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "xArtifact artf1265 created successfully at the bottom of the planning folder"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "i.icon-edit"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "title"
      },
      "text": "test002 - updated"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Update"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "xArtifact artf1265 updated successfully"
    }
  ]
}